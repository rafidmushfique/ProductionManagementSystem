    function GetGRNData(e) {
        var eid = e.id.split('_');
        let selectionId = eid[0] + '_' + eid[1] + '_' + 'GRNNo';
        let floorstockRowId = eid[0] + '_' + eid[1] + '_' + 'FloorStock';
        let requisitionNo = e.value;
        let materialCode = e.dataset.id;
        let rowid = "#" + selectionId;
        $(rowid).empty();

        $.ajax({
            url: '/Production/GetGRNList',
            method: 'GET',
            data: { 
                RequisitionNo: requisitionNo,
                MaterialCode: materialCode
            }, 
            success: function (result) {
              
                // Handle the successful response here
                $.each(result, function (idx, item) {
                    //console.log(item);
                    var o = new Option(item.Grnno, item.Grnno);
                    $(o).html();
                    document.getElementById(selectionId).append(o);
                    //document.getElementById(floorstockRowId).vlaue = item.AvailableStock;

                });
              
            },
            error: function (xhr, status, error) {
                // Handle errors
                console.error('AJAX request failed:', error);
            }
        });
    
    }
