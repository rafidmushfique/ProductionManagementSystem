    function removeRowItem(id) {
        var controlToBeRemoved = "#trtbProductionBOMDtl" + id;
        //alert(controlToBeRemoved);
        $(controlToBeRemoved).remove();
        $("#tblProductionBOMDtl").children("tr").each(function (i) {
           
           
            var oldRowId = this.id;
            var rowId = oldRowId.replace(/\d/g, '');
            var newRowId = rowId + i;

            console.log("old: "+oldRowId);
            console.log("new: "+newRowId);
            $(this).attr("id", newRowId);
            updateInputAttributes($(this), i);
        });
    }

    function updateInputAttributes(row, newIndex) {
        // Update the ID and name attributes for various input fields based on the new index
        var prefix = "tblRebProductionProcessDetail[" + newIndex + "]";

        row.find("[id^='tblRebProductionProcessDetail_']").each(function () {

            var oldId = $(this).attr("id");
            var oldName = $(this).attr("name");
            var newId = oldId.replace(/tblRebProductionProcessDetail_\d+/, 'tblRebProductionProcessDetail_' + newIndex);
   
            $(this).attr("id", newId);
            $(this).attr("name", prefix +"." +oldId.substring(oldId.lastIndexOf("_") + 1));
            // debugger;
           
           
        });
    }
